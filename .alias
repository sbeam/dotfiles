alias ll='ls -lF'
alias la='ls -laF'
alias lt='ls -alt | head -20'
alias ld='ls -al -d * | egrep "^d"'
alias li='ls -ai1 | sort'
alias pss='ps -axf'
#alias \+='pushd .'
#alias \_='popd'
alias dusk='du -s -m -c * | sort -n'
alias nsl='netstat -alnp --protocol=inet | grep -v CLOSE_WAIT | cut -c-6,21-94 | tail +2'
alias h='history'
alias randpw='head -c 10 /dev/random | base64 - | cut -c -8'
alias screen='TERM=screen screen'
# alias vim='mvim -v'
alias vi='vim'
alias vimenc='vim -x --cmd "set cryptmethod=blowfish2" --cmd "set backup" --cmd "set noswapfile" --cmd "set viminfo=\"\"" -u NONE'
alias mustard='vim ~/tmp/mustard.gpg'
# alias ketchup='vim --cmd "set backup" --cmd "set noswapfile" --cmd "set viminfo=\"\"" -u NONE ~/Dropbox\ \(Personal\)/ketchup'
alias be='bundle exec'
alias befr='bundle exec foreman run'
alias bets='bundle exec thin start'
alias befs='bundle exec foreman start -f Procfile.development'
alias chrome_suffer='open /Applications/Google\ Chrome.app --args --disable-web-security'
alias vimswapclean='find . -name \*.sw[po] -exec rm {} \;'
alias nuke_test_db='bundle exec rake db:migrate:reset db:seed RAILS_ENV=test'
alias nuke_pumas="lsof -i tcp:3000 | awk '{print \$2}' | tail +2 | xargs kill"
